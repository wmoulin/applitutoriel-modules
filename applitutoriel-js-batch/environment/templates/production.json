{
  "contextPath": "#{APPLICATION_CONTEXT_ROOT}",
  "localeI18n": {
    "locale": "fr-FR",
    "lang": "fr"
  },
  "database": {
    "config": {
      "uri": "#{DATABASE_URI}",
      "options" : {
        "pool" : {
          "max" : 15,
          "min" : 0,
          "idle" : 1000
        },
        "define": {
          "timestamps" : false
        },
        "loggingLevel" : "INFO"
      }
    }
  },
  "mock": {
    "enabled": false
  },
  "fullSpa": {
    "enabled": false
  },
  "cache": {
    "enabled": false
  }
}
